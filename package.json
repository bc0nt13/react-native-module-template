{
  "name": "react-native-module-template",
  "version": "1.1.0",
  "description": "A starter for the React Native library written in TypeScript, with linked example project and optional native code",
  "homepage": "https://github.com/bc0nt13/react-native-module-template#readme",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Bruno Conti <bruno.conti@taplabs.rocks>",
  "license": "MIT",
  "files": [
    "android",
    "ios",
    "lib",
    "react-native-module-template.podspec",
    "!.DS_Store",
    "!.gradle",
    "!.idea",
    "!build",
    "!gradle",
    "!*.iml",
    "!gradlew",
    "!gradlew.bat",
    "!local.properties",
    "!project.xcworkspace",
    "!xcshareddata",
    "!xcuserdata"
  ],
  "scripts": {
    "compile": "tsc -p .",
    "lint": "eslint .",
    "prepare": "yarn compile",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.62.12",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.0.1",
    "eslint": "^7.1.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "react": "^16.13.1",
    "react-native": "^0.62.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
